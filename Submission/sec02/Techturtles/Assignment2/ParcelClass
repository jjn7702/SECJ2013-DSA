class Parcel
{
private:
    string trackingNumber, address, nameSender, nameReceiver;
    char category;
    int status;
    int count; // edit

public:
    Parcel(string tn, string a, string nSend, string nRec, char c, int s)
    {
        trackingNumber = tn;
        address = a;
        nameSender = nSend;
        nameReceiver = nRec;
        category = c;
        status = s;
    }

void printResult(int count)
    {
        cout << "\t\t\t" << left << setw(5) << count 
		 << setw(15) << trackingNumber
		 << setw(2) << address
		 << setw(2) << nameSender
		 << setw(2) << nameReceiver
		 << setw(2) << category
		 << setw(2) << status << endl;
    }
